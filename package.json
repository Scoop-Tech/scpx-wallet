{
    "name": "scpx-wallet",
    "version": "0.9.17",
    "description": "Scoop Core Wallet: multi-asset, cross-platform and open-source crypto wallet",
    "keywords": [
        "crypto wallet",
        "crypto",
        "wallet",
        "cryptocurrency",
        "bitcoin",
        "segwit",
        "btc",
        "ethereum",
        "eth",
        "scoop",
        "litecoin",
        "ltc",
        "digibyte",
        "dgb",
        "erc20",
        "trueusd",
        "tusd",
        "tether",
        "usdt",
        "chainlink",
        "link",
        "swap",
        "nodejs",
        "node",
        "js"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Scoop-Tech/scpx-wallet"
    },
    "homepage": "https://scoop.tech",
    "bugs": "https://github.com/Scoop-Tech/scpx-wallet/issues",
    "main": "sw-cli.js",
    "scripts": {
        "help": "node --experimental-worker ./sw-cli.js --help ",
        "about": "set NODE_ENV=development&& node --inspect=0.0.0.0:9229 ./sw-cli.js --about",
        "dev": "set NODE_ENV=development&& node --inspect=0.0.0.0:9229 ./sw-cli.js --saveHistory=true",
        "start": "set NODE_ENV=production&& node --inspect=0.0.0.0:9229 ./sw-cli.js --saveHistory=false",
        "rpc": "set NODE_ENV=production&& node --inspect=0.0.0.0:9229 ./sw-cli.js --rpc=true --rpcPort=4000 --rpcRemoteHosts=::ffff:127.0.0.1 --rpcUsername=scp --rpcPassword=123",
        "jestver": "set NODE_ENV=test&& jest --version",
        "test": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-worker jest --runInBand --forceExit --verbose --coverage --env=node"
    },
    "jest": {
        "coverageDirectory": "./coverage/",
        "collectCoverage": true
    },
    "engineStrict": true,
    "engines": {
        "node": "11.10.1",
        "npm": "6.7.0"
    },
    "author": "Dominic Morris",
    "license": "AGPL-3.0",
    "dependencies": {
        "axios": "^0.19.0",
        "base32-encode": "^1.1.1",
        "bchaddrjs": "^0.3.2",
        "bignumber.js": "^8.0.2",
        "bip32": "^1.0.4",
        "bip65": "^1.0.3",
        "bip68": "^1.0.3",
        "bitcoinjs-lib": "5.2.0",
        "bitgo-utxo-lib": "1.8.0",
        "chalk": "^2.4.2",
        "circular-buffer": "^1.0.2",
        "clear": "^0.1.0",
        "coininfo": "^5.1.0",
        "colors": "^1.3.3",
        "commander": "^2.20.0",
        "console-title": "^1.1.0",
        "crypto-js": "^4.0.0",
        "detect-node": "^2.0.4",
        "dirty": "^1.1.0",
        "dotenv": "^8.2.0",
        "eosjs": "16.0.9",
        "eosjs-keygen": "1.3.2",
        "ethereum-input-data-decoder": "^0.2.0",
        "ethereumjs-tx": "^1.3.7",
        "ethereumjs-util": "^5.2.0",
        "figlet": "^1.2.1",
        "isomorphic-ws": "^4.0.1",
        "jayson": "^3.0.2",
        "js-big-decimal": "^1.2.1",
        "json-stringify-safe": "^5.0.1",
        "localforage": "^1.7.3",
        "lodash": "^4.17.15",
        "minimist": "^1.2.0",
        "moment": "^2.24.0",
        "p-limit": "^2.2.0",
        "read-last-lines": "^1.7.1",
        "redux": "^3.6.0",
        "redux-batched-actions": "^0.4.1",
        "redux-persist": "^5.10.0",
        "redux-thunk": "^2.1.0",
        "repl.history": "^0.1.4",
        "scp-address-validator": "^0.2.8",
        "socket.io-client": "2.2.0",
        "tiny-secp256k1": "^1.1.6",
        "triple-beam": "^1.3.0",
        "web3": "2.0.0-alpha.1",
        "winston": "^3.2.1"
    },
    "bin": {
        "sw-cli": "./sw-cli.js"
    },
    "devDependencies": {
        "codecov": "^3.3.0",
        "cross-env": "^7.0.3",
        "jest": "21.2.1",
        "nodemon": "^1.19.0"
    }
}
